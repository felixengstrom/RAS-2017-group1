<launch>
    <node name="left_motor" pkg="phidgets" type="motor">
        <param name="serial" type="int" value="475517" />
	<param name="name" type="string" value="left_motor" />
    </node>
    <node name="right_motor" pkg="phidgets" type="motor">
        <param name="serial" type="int" value="475398" />
	<param name="name" type="string" value="right_motor" />
    </node>
    <node name="motor_controller" pkg="motor_controller" type="motor_controller" output="screen">
    <param name="proportion_left" type="double" value="0.0"/>
    <param name="proportion_right" type="double" value="0.0"/>
    <param name="alpha_left" type="double" value="2.5"/>
    <param name="alpha_right" type="double" value="3.0"/>
    <param name="beta_left" type="double" value="5.0"/>
    <param name="beta_right" type="double" value="5.0"/>
    </node>
</launch>

